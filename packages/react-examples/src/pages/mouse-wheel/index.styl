.mouse-wheel-vertical-scroll
  .mouse-wheel-wrapper
    height 400px
    overflow hidden
    .mouse-wheel-item
      height 50px
      line-height 50px
      font-size 20px
      font-weight bold
      text-align center
      &:nth-child(2n)
        background-color #C3D899
      &:nth-child(2n+1)
        background-color #F2D4A7
