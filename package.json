{
  "name": "better-scroll",
  "version": "2.0.0",
  "description": "inspired by iscroll, and it has a better scroll perfermance",
  "author": {
    "name": "huangyi",
    "email": "ustbhuangyi@gmail.com"
  },
  "main": "dist/bscroll.js",
  "module": "dist/bscroll.esm.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "dev": "webpack-dev-server --config ./build/example/webpack.dev.conf.js --host 0.0.0.0 --port 8932",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "node build/build.js",
    "build:plugin": "node build/build.js --special plugins",
    "build:demo": "node build/build-demo.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "cm": "git-cz",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "travis-deploy-once": "travis-deploy-once",
    "docs:dev": "vuepress dev vuepress-docs"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
